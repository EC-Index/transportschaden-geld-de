{
  "cleanUrls": true,
  "trailingSlash": true,
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "transportschaden-geld.de"
        }
      ],
      "destination": "https://schaden-bezahlt.de/$1",
      "permanent": true
    },
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "www.transportschaden-geld.de"
        }
      ],
      "destination": "https://schaden-bezahlt.de/$1",
      "permanent": true
    }
  ]
}